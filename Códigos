
select  
        mp.organization_code "COD ORGANIZACAO", 
        oohv.order_number "Ordem",
        oohv.ORDER_TYPE "Tipo da Ordem",
         oola.line_number || '.' || oola.shipment_number "LINHA RMA"  , 
        oola.ORDERED_ITEM "Item",
        oola.ORDERED_QUANTITY "Quantidade", 
        oola.ORDER_QUANTITY_UOM "UDM",
        oola.PRICING_QUANTITY "Valor Total Linha",
        rcta.INVOICE_CURRENCY_CODE "Moeda",
        oohv.SOLD_TO "CLIENTE",
        oohv.booked_date "DATA ENTRADA",
        oohv.ATTRIBUTE10 "NOTA FISCAL ORIGEN",
        --rbsa.NAME "NOTA FISCAL ORIGEN"
        oohv.ATTRIBUTE12 "DATA NF ORIGEM" ,
        oohv.ATTRIBUTE3 "Gerar NF e entrada de recusa?",
        oohv.ATTRIBUTE16 "Justificativa"
       
        
from    apps.OE_ORDER_HEADERS_V oohv,
        apps.OE_ORDER_LINES_all oola,
        apps.ra_batch_sources_all rbsa,
        apps.ra_customer_trx_all rcta,
        apps.ra_customer_trx_lines_all rctla,
        mtl_parameters   mp

where   oohv.order_number = 829390 and
        oohv.order_category_code = 'RETURN' AND
        oohv.HEADER_ID = oola.HEADER_ID and
        oola.ship_from_org_id = mp.organization_id and
        rcta.BATCH_SOURCE_ID = rbsa.BATCH_SOURCE_ID and
        rcta.customer_trx_id = rctla.customer_trx_id AND
        rctla.customer_trx_line_id = oola.reference_customer_trx_line_id and
        rcta.customer_trx_id = rctla.customer_trx_id AND
        trunc(ATTRIBUTE12) = to_date('DD/MM/YYYY, DD/MM/YYYY') group by to_char (ATTRIBUTE12) 
        oohv.creation_Date  BETWEEN
        TO_DATE('01/01/2023', 'DD/MM/YYYY') AND
        TO_DATE('30/08/2024', 'DD/MM/YYYY') 
        ORDER BY oohv.creation_Date  DESC  
